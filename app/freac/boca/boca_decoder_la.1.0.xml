<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE component SYSTEM "http://www.freac.org/boca/component.dtd">
<component>
  <name>La Lossless Audio Decoder</name>
  <version>1.0</version>
  <id>la-dec</id>
  <type>decoder</type>
  <format>
    <name>La Audio Files</name>
    <lossless>true</lossless>
    <extension>la</extension>
    <tag id="id3v1-tag" mode="append">ID3v1</tag>
  </format>
  <external>
    <command>la</command>
    <arguments>-cout %OPTIONS %INFILE</arguments>
    <mode>stdio</mode>
    <outformat>wave</outformat>
  </external>
</component>
